@model IEnumerable<TrendingViewModel>

<div class="row">
    <div class="col-lg-12">
        <div class="trending-tittle">
            <strong>Trend xəbərlər</strong>
            <!-- <p>Rem ipsum dolor sit amet, consectetur adipisicing elit.</p> -->
            <div class="trending-animated">
                <ul id="js-news" class="js-hidden">
                    @foreach (var item in Model)
                    {
                    <li class="news-item"><a asp-controller="News" asp-action="Details" asp-route-id="@item.Id">@item.Title</a> </li>
                    }
                </ul>
            </div>

        </div>
    </div>
</div>